<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EEA3EAE0">


    <RelativeLayout
        android:layout_width="367dp"
        android:layout_height="48dp"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="132dp"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/icon3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="339dp"
            android:layout_marginBottom="12dp"
            android:src="@drawable/setting_search" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="62dp"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/yaldevi"
            android:text="Mục đích"
            android:textColor="#000000"
            android:textStyle="bold"
            android:background="@drawable/rounded_view_cafe"
            android:padding="5dp"/>

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="144dp"
            android:layout_marginTop="12dp"
            android:text="Khoảng cách"
            android:fontFamily="@font/yaldevi"
            android:textColor="#000000"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="22dp"
            android:text="Đang mở cửa"
            android:fontFamily="@font/yaldevi"
            android:textColor="#000000"
            android:textStyle="bold"/>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>